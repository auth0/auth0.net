{
  "metadata": [
    {
      "src": [
        {
          "files": [ "Auth0.Core/**/*.cs", "Auth0.AuthenticationApi/**/*.cs", "Auth0.ManagementApi/**/*.cs" ],
          "exclude": [ "**/bin/**", "**/obj/**" ],
          "cwd": "../src"
        }
      ],
      "dest": "obj/api"
    }
  ],
  "build": {
    "force": false,
    "content": [
      {
        "files": [ "**/*.yml" ],
        "src": "obj/api",
        "dest": "api"
      },
      {
        "files": [ "articles/**/*.md", "*.md", "toc.yml" ]
      }
    ],
    "resource": [
      {
        "files": [
          "images/**"
        ],
        "case": false,
        "noNegate": false,
        "noExpand": false,
        "noEscape": false,
        "noGlobStar": false,
        "dot": false
      }
    ],
    "externalReference": [],
    "dest": "../docs",
    "template": [
      "default"
    ],
    "globalMetadata": {
      "_appLogoPath": "images/logo.svg",
      "_appFaviconPath": "images/logo.png"
    },
    "serve": false
  }
}